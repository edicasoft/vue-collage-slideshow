{
  "name": "vue-collage-slideshow",
  "description": "Responsive slideshow with animated collages for Vue.js",
  "version": "0.2.7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edicasoft/vue-collage-slideshow.git"
  },
  "keywords": [
    "vue",
    "slideshow",
    "collage"
  ],
  "author": "Daria Mykhailova",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name VueCollageSlideshow"
  },
  "dependencies": {
    "@flockos/vue-components": "^0.1.73",
    "vue": "^2.6.12",
    "vue-carousel": "^0.17.0",
    "vue-gallery": "^1.5.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^4.5.11",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "./dist/VueCollageSlideshow.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "license": "MIT"
}
